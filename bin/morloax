#!/usr/bin/env php
<?php
require_once __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;

$app = new Application();
$app->add(new Commands\GenerateSettings());
$app->add(new Commands\Init());
$app->add(new Commands\GenerateModel());
try {
  $app->run();
} catch (Exception $e) {
    die($e);
}